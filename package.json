{
  "name": "pipepack-tapable",
  "version": "0.1.0",
  "description": "yet, reimplement tapable with idea 'simple is the best'.",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prepublishOnly": "npm-run-all -s clean compile",
    "test": "jest --coverage",
    "test:cov": "jest --watch",
    "clean": "rm -rf lib",
    "compile": "babel src --source-maps --out-dir lib --extensions '.js,.ts'",
    "compile:watch": "babel src --source-maps --out-dir lib --extensions '.js,.ts' --watch"
  },
  "files": [
    "lib",
    "native"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pipepack/pipepack-tapable.git"
  },
  "keywords": [
    "nodejs",
    "universal"
  ],
  "author": "huang.jian <hjj491229492@hotmail.com>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pipepack/pipepack-tapable/issues"
  },
  "homepage": "https://github.com/pipepack/pipepack-tapable#README",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cz-conventional-changelog": "3.1.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "husky": "^4.0.2",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.0",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}